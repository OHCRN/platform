datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

model OhipKey {
  participantId     String @unique
  ohipPrivateKey    String @id @unique @default("") @db.Char(21)
}

model ClinicalProfileKey {
  participantId             String @unique
  clinicalProfilePrivateKey String @id @unique @default("") @db.Char(21)
}
