datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

model Ohip {
  ohipPrivateKey String @id @db.Char(21)
  ohipNumber     String @unique
}

