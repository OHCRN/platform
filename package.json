	"main": "index.js",
	"scripts": {
		"build": "pnpm -r run build",
		"consent-ui-dev": "pnpm --filter consent-ui run dev",
		"consent-api-dev": "pnpm --filter consent-api run dev",
		"das-dev": "pnpm --filter '*-das' run dev",
		"data-mapper-dev": "pnpm --filter data-mapper run dev",
		"dev": "pnpm --parallel run dev",
		"test": "vitest",
		"migrate-dev": "pnpm --filter '*-das' run migrate-dev",
		"generate": "pnpm --filter '*-das' --workspace-concurrency=1 run generate",
		"seed": "pnpm --filter '*-das' run seed",
		"start": "pnpm run migrate-dev && pnpm run build && pnpm run dev"
	},
