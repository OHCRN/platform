		"prettier": "^2.8.8",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2",
		"vitest": "^0.34.6"
	},
	"pnpm": {
		"overrides": {
			"@typescript-eslint/parser": "6.2.0",
			"eslint-plugin-import": "^2.27.5"
